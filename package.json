{
	"name": "react-story-tree",
	"version": "0.1.0",
	"description": "A React library for building and analyzing branching narrative quest systems with LLM-powered story critique",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": [
		"react",
		"quest",
		"narrative",
		"story",
		"tree",
		"branching",
		"llm",
		"ai",
		"analysis",
		"game",
		"gamification",
		"visualization"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tommyshellberg/react-story-tree.git"
	},
	"bugs": {
		"url": "https://github.com/tommyshellberg/react-story-tree/issues"
	},
	"homepage": "https://github.com/tommyshellberg/react-story-tree#readme",
	"peerDependencies": {
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0",
		"@mui/material": "^5.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"dependencies": {
		"@xyflow/react": "^12.9.0",
		"ai": "^4.3.19",
		"dagre": "^0.8.5"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/dagre": "^0.7.53",
		"@types/node": "^22.18.12",
		"@types/react": "^18.3.26",
		"@types/react-dom": "^18.3.7",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/ui": "^2.1.9",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.2",
		"jsdom": "^25.0.1",
		"prettier": "^3.6.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3",
		"vitest": "^2.1.9"
	}
}