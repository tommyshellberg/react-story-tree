{
  "name": "react-quest-tree",
  "version": "0.1.0",
  "description": "A React library for building and analyzing branching narrative quest systems with LLM-powered story critique",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "react",
    "quest",
    "narrative",
    "story",
    "tree",
    "branching",
    "llm",
    "ai",
    "analysis",
    "game",
    "gamification",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-quest-tree.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-quest-tree/issues"
  },
  "homepage": "https://github.com/yourusername/react-quest-tree#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@mui/material": "^5.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "dependencies": {
    "@xyflow/react": "^12.8.4",
    "ai": "^4.0.0",
    "dagre": "^1.1.5"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
