#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/vitest@2.1.9_@types+node@22.18.12_@vitest+ui@2.1.9_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/vitest@2.1.9_@types+node@22.18.12_@vitest+ui@2.1.9_jsdom@25.0.1/node_modules:/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/vitest@2.1.9_@types+node@22.18.12_@vitest+ui@2.1.9_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/vitest@2.1.9_@types+node@22.18.12_@vitest+ui@2.1.9_jsdom@25.0.1/node_modules:/Users/thomasshellberg/Projects/unquest/react-quest-tree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
